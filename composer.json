    "description": "A captcha componet for hyperf .",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "captcha"
    ],
    "require": {
        "php": ">=8.0",
        "psr/container": "^1.0|^2.0",
        "hyperf/session": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
            "Lizhaoyang\\Captcha\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.1-dev"
        },
        "hyperf": {
            "config": "Hyperf\\Captcha\\ConfigProvider"
        }
    }
}
